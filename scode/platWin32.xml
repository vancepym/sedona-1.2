<!--
  -  Sedona SCode manifest for a win32 platform
  -->

<sedonaCode
   endian="little"
   blockSize="4"
   refSize="4"
   main="sys::Sys.main"
   debug="true"
   test="true"
>

  <!-- Kits -->
  <depend on="sys 1.0+"           test="true"/>
  <depend on="sox 1.0+"           test="true"/>
  <depend on="inet 1.0+"          test="true"/>
  <depend on="web 1.0+"           test="true"/>
  <depend on="control 1.0+"       test="true"/>
  <depend on="pstore 1.0+"        test="true"/>
  <depend on="datetime 1.0+"      test="true"/>
  <depend on="datetimeStd 1.0+"   test="true"/>
  <depend on="basicSchedule 1.0+" test="true"/>
  <depend on="platWin32 1.0+"     test="true"/>

</sedonaCode>

