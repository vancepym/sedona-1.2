  <depend on="pstore 1.0+" />
  <depend on="datetime 1.0+" />
  <depend on="datetimeStd 1.0+" />
  <depend on="basicSchedule 1.0+" />
  <depend on="platWin32 1.0+" />

</sedonaCode>

