class PrioBoolTest
  extends Test
{
  static inline PrioritizedBool  b

 
  static void test()
  {
    // Check output & sourceLevel for defaults
    assert(b.out==false)                              // uninitialized bool should be false
    assert(b.sourceLevel==PrioritizedBool.MAXLEVEL)   // uninitialized level is MAXLEVEL

    // Setting inputs directly won't work, since prioritized design only looks at links
    b.in1 = true 
    assert(b.out==false)
    assert(b.sourceLevel==PrioritizedBool.MAXLEVEL)

    // Use actions to set in1 & in8 & fallback (need links to test other levels)






  }

}
